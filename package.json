{
  "private": true,
  "name": "bare-expo",
  "description": "Example of embedding Bare in an Expo application",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "gen:bundle": "npx bare-pack --target ios-arm64 --target ios-arm64-simulator --target ios-x64-simulator --target android-arm --target android-arm64 --target android-ia32 --target android-x64 --linked --out app/app.bundle.js backend/backend.js",
    "gen:schema": "node backend/schema.js",
    "fix": "npx standard --fix",
    "test": "npx standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holepunchto/bare-expo.git"
  },
  "author": "Holepunch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/holepunchto/bare-expo/issues"
  },
  "standard": {
    "globals": [
      "Bare",
      "Pear",
      "Barekit",
      "BareKit"
    ]
  },
  "homepage": "https://github.com/holepunchto/bare-expo#readme",
  "dependencies": {
    "b4a": "^1.6.7",
    "bare-stream": "^2.6.5",
    "events": "^3.3.0",
    "expo": "^53.0.9",
    "expo-build-properties": "^0.14.6",
    "expo-constants": "^17.1.6",
    "expo-linking": "^7.1.5",
    "expo-router": "^5.0.6",
    "expo-system-ui": "^5.0.7",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "react-native": "^0.79.2",
    "react-native-bare-kit": "^0.8.1",
    "react-native-safe-area-context": "^5.4.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.3",
    "prettier": "^3.3.3",
    "prettier-config-standard": "^7.0.0",
    "typescript": "^5.3.3"
  }
}
